<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="model"
            type="com.dyyj.idd.chatmore.model.network.result.CashSumaryResult.Data" />
    </data>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/cl_layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="0dp">

                <LinearLayout
                    android:id="@+id/ll_4"
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="30dp"
                    android:layout_marginStart="30dp"
                    android:background="@drawable/img_withdrawal1"
                    android:orientation="horizontal"
                    android:gravity="center">

                </LinearLayout>

                <ImageView
                    android:id="@+id/bg_iv"
                    android:layout_width="330dp"
                    android:layout_height="85dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="22dp"
                    android:src="@drawable/img_withdrawal7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_4"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/icon_iv"
                    android:layout_width="69dp"
                    android:layout_height="69dp"
                    android:layout_marginStart="15dp"
                    android:src="@drawable/ic_money_big"
                    app:layout_constraintBottom_toBottomOf="@+id/bg_iv"
                    app:layout_constraintStart_toStartOf="@+id/bg_iv"
                    app:layout_constraintTop_toTopOf="@+id/bg_iv" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="@+id/icon_iv"
                    app:layout_constraintStart_toEndOf="@+id/icon_iv"
                    app:layout_constraintTop_toTopOf="@+id/icon_iv">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="当前余额"
                        android:textColor="#ff573579"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/balance_value_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#66410B"
                        android:textSize="36sp" />
                </LinearLayout>

                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/withdraw_btn"
                    android:layout_width="100dp"
                    android:layout_height="37dp"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/rect_rounded_left_right_arc5"
                    android:text="提现"
                    android:textColor="#8B5107"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/bg_iv"
                    app:layout_constraintEnd_toEndOf="@+id/bg_iv"
                    app:layout_constraintTop_toTopOf="@+id/bg_iv" />

                <ImageView
                    android:id="@+id/imageView49"
                    android:layout_width="330dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="@+id/bg_iv"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/bg_iv"
                    app:layout_constraintTop_toBottomOf="@+id/bg_iv"
                    app:srcCompat="@drawable/img_withdrawal4"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/tv_social_tip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="连续登录3天，可提现1元"
                    android:textColor="#8B5107"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView49"
                    app:layout_constraintEnd_toEndOf="@+id/imageView49"
                    app:layout_constraintStart_toStartOf="@+id/imageView49"
                    app:layout_constraintTop_toTopOf="@+id/imageView49"
                    tools:ignore="MissingConstraints" />
            </android.support.constraint.ConstraintLayout>


            <android.support.constraint.ConstraintLayout
                android:id="@+id/cl_layout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@+id/cl_layout1"
                tools:ignore="MissingConstraints"
                tools:layout_editor_absoluteX="0dp">

                <LinearLayout
                    android:id="@+id/ll_5"
                    android:layout_width="match_parent"
                    android:layout_height="12dp"
                    android:gravity="center"
                    android:layout_marginEnd="30dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:background="@drawable/img_withdrawal5"
                    tools:layout_editor_absoluteX="20dp"
                    tools:layout_editor_absoluteY="2dp">

                </LinearLayout>

                <ImageView
                    android:id="@+id/bg_iv2"
                    android:layout_width="330dp"
                    android:layout_height="85dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="22dp"
                    android:src="@drawable/img_withdrawal6"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_5"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/icon_iv2"
                    android:layout_width="69dp"
                    android:layout_height="69dp"
                    android:layout_marginStart="15dp"
                    android:src="@drawable/ic_money_big"
                    app:layout_constraintBottom_toBottomOf="@+id/bg_iv2"
                    app:layout_constraintStart_toStartOf="@+id/bg_iv2"
                    app:layout_constraintTop_toTopOf="@+id/bg_iv2" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:orientation="vertical"
                    app:layout_constraintBottom_toBottomOf="@+id/icon_iv2"
                    app:layout_constraintStart_toEndOf="@+id/icon_iv2"
                    app:layout_constraintTop_toTopOf="@+id/icon_iv2">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="当前余额"
                        android:textColor="#ff573579"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/balance_value_tv2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#66410B"
                        android:textSize="36sp" />
                </LinearLayout>

                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/withdraw_btn2"
                    android:layout_width="100dp"
                    android:layout_height="37dp"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/rect_rounded_left_right_arc5"
                    android:text="提现"
                    android:textColor="#8B5107"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/bg_iv2"
                    app:layout_constraintEnd_toEndOf="@+id/bg_iv2"
                    app:layout_constraintTop_toTopOf="@+id/bg_iv2" />

                <ImageView
                    android:id="@+id/imageView50"
                    android:layout_width="330dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    app:layout_constraintEnd_toEndOf="@+id/bg_iv2"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="@+id/bg_iv2"
                    app:layout_constraintTop_toBottomOf="@+id/bg_iv2"
                    app:srcCompat="@drawable/img_withdrawal3"
                    tools:ignore="MissingConstraints" />

                <TextView
                    android:id="@+id/tv_invite_tip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:text="邀请1人注册（立得两元），可提现1元现金"
                    android:textColor="#8B5107"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView50"
                    app:layout_constraintEnd_toEndOf="@+id/imageView50"
                    app:layout_constraintStart_toStartOf="@+id/imageView50"
                    app:layout_constraintTop_toTopOf="@+id/imageView50"
                    tools:ignore="MissingConstraints" />
            </android.support.constraint.ConstraintLayout>


            <ImageView
                android:id="@+id/iv_bg2"
                android:layout_width="330dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="18dp"
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/img_withdrawal8"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cl_layout2"
                app:layout_constraintVertical_bias="0.09"
                tools:ignore="MissingConstraints" />


        </android.support.constraint.ConstraintLayout>
    </android.support.v4.widget.NestedScrollView>
</layout>